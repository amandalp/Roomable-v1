<html>
<head>
	<title><%= room.title %></title>
</head>
<body>
	<h1><%= room.title %></h1>
	<form action="/rooms/<%= room.id %>?_method=PATCH" method="POST">
		<input type="text" name="title" value="<%= room.title %>">
		<input type="text" name="neighborhood" value="<%= room.neighborhood %>">
		<input type="text" name="price" value="<%= room.price %>">
		<input type="text" name="open" value="<%= room.open %>">
		<textarea name"description"><%= room.description %></textarea>
		<input type="submit">
	</form>
</body>
</html>